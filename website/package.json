{
  "version": "0.1.25",
  "scripts": {
    "start": "docusaurus start",
    "build": "npm run type-check && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "watch-tests": "mocha --require ts-node/register --watch --watch-files src, ./src/**/*.test.ts",
    "type-check": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.65",
    "@docusaurus/preset-classic": "^2.0.0-alpha.65",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@icgc-argo/uikit": "^1.7.0",
    "axios": "^0.19.0",
    "clipboard": "^2.0.4",
    "docsearch.js": "^2.6.3",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-zoom-pan-pinch": "^1.6.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.65",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.2",
    "@types/react": "^16.9.53",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "chai": "^4.2.0",
    "commander": "^4.1.0",
    "dotenv-webpack": "^1.7.0",
    "husky": "^4.3.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "svg-inline-loader": "^0.8.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run type-check"
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
